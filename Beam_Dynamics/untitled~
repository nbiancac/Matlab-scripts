DataDir='/afs/cern.ch/user/n/nbiancac/ln_work/scratch0/IRIS/Getting_LHC_beta_functions/2015/';
opt1='LHC-ft';
name1='twiss.lhc.b1.ft6.5tev.thick_beta_elements.dat';
opt2='LHC-squeeze50cm';
name2='twiss.lhc.b1.coll6.5tev_50cm.thick_beta_elements.dat';

L1=importdata([DataDir,'/',opt1,'/',name1]);
L2=importdata([DataDir,'/',opt2,'/',name2]);

figure(1)
plot(L1.data(:,1)); hold on;
plot(L2.data(:,1),'r'); hold on;

